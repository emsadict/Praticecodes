// SELECTING WHATS IN A TABLE THAT IS NOT IN ANOTHER TABLE

SELECT * FROM
 `spgs_basicinfo` WHERE `regno`
 NOT IN (SELECT `regno` FROM `admitted_2022_2023`) 


// OR 
SELECT name 
FROM table2 
WHERE NOT EXISTS 
    (SELECT * 
     FROM table1 
     WHERE table1.name = table2.name)


