SELECT * FROM `course_reg` WHERE `faculty`='Sciences' AND `dept` LIKE 'Biological Sciences%' AND `semester`='FIRST'



//counting registration by programmes

SELECT `dept`,`programme`, COUNT(`programme`) FROM `spgs_basicinfo` GROUP by `programme`,`dept`;


//Fetching
SELECT * FROM `course_regp` WHERE `faculty`='Sciences' AND `dept` LIKE 'Chemistry%' AND `level`='800' and `semester` LIKE 'SECOND%'


